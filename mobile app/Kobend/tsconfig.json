{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.jsx",
    "app/_layout.jsx",
    "app/(auth)/signup.jsx",
    "app/(auth)/signin.jsx",
    "nativewind-env.d.ts"
, "app/(pages)/profile/index.jsx", "api/devices/subscription.js"  ]
}